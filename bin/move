#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Define starting board - empty
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"
puts "Where would you like to go?"
input = gets.strip
input_to_index(input)
move(board, 0, "X")
display_board(board)
